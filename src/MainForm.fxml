<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainFormController">
   <Button fx:id="btnClick" layoutX="59.0" layoutY="98.0" mnemonicParsing="false" onAction="#onClickonAction" prefHeight="37.0" prefWidth="159.0" text="Click Here" textFill="#39e558">
      <font>
         <Font name="Segoe UI Bold" size="15.0" />
      </font>
   </Button>
   <TextField fx:id="txtUsername" layoutX="35.0" layoutY="44.0" prefHeight="25.0" prefWidth="280.0" promptText="Enter your Name">
      <font>
         <Font size="14.0" />
      </font>
   </TextField>
   <TextField fx:id="txtPrint" layoutX="66.0" layoutY="170.0" prefHeight="30.0" prefWidth="152.0" />
   <TextField fx:id="txtNum1" layoutX="26.0" layoutY="258.0" promptText="Enter Number 1" />
   <TextField fx:id="txtNum2" layoutX="205.0" layoutY="258.0" promptText="Enter Number 2" />
   <TextField fx:id="txtResult" layoutX="388.0" layoutY="258.0" />
   <Button fx:id="btnAdd" layoutX="218.0" layoutY="316.0" mnemonicParsing="false" onAction="#addTwoNumbers" text="Add" />
   <Button fx:id="btnClear" layoutX="300.0" layoutY="316.0" mnemonicParsing="false" onAction="#btnClear" text="Clear" />
</AnchorPane>
